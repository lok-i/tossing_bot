<mujoco model="arm3">
    <actuator>
        <!-- Physical limits of the actuator. Pulled directly from datasheet. See pg 24 of:
         https://www.kinovarobotics.com/sites/default/files/UG-014_KINOVA_Gen3_Ultra_lightweight_robot_User_guide_EN_R06_0.pdf -->
        <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="arm3/Actuator1" name="arm3/torq_j1"/>
        <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="arm3/Actuator2" name="arm3/torq_j2"/>
        <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="arm3/Actuator3" name="arm3/torq_j3"/>
        <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="arm3/Actuator4" name="arm3/torq_j4"/>
        <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="arm3/Actuator5" name="arm3/torq_j5"/>
        <motor ctrllimited="true" ctrlrange="-13.0 13.0" joint="arm3/Actuator6" name="arm3/torq_j6"/>
        <motor ctrllimited="true" ctrlrange="-13.0 13.0" joint="arm3/Actuator7" name="arm3/torq_j7"/>
   
        <adhesion name="arm3/adhere_arm" body="arm3/4boxes" ctrlrange="0 1" gain="5"/>
    </actuator>



    <worldbody>

    
        <body name="arm3/base" pos="0 1.0 0">
            <!-- robot view -->
            <camera mode="fixed" pos="1.0 0 0.4" quat="0.653 0.271 0.271 0.653"/>
            <inertial diaginertia="0 0 0" mass="0" pos="0 0 0"/>
            <!-- mount attached here -->
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" mesh="base_link" />
            <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="base_link" name="arm3/base_collision" />
            <body name="arm3/shoulder_link" pos="0 0 0.15643" quat="-3.67321e-06 1 0 0">
                <inertial pos="-2.3e-05 -0.010364 -0.07336" quat="0.707051 0.0451246 -0.0453544 0.704263" mass="1.3773" diaginertia="0.00488868 0.00457 0.00135132" />
                <joint name="arm3/Actuator1" pos="0 0 0" axis="0 0 1" damping="0.1" />
                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" name="arm3/s_visual" mesh="shoulder_link" />
                <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="shoulder_link" name="arm3/s_collision"/>
                <body name="arm3/HalfArm1_Link" pos="0 0.005375 -0.12838" quat="0.707105 0.707108 0 0">
                    <inertial pos="-4.4e-05 -0.09958 -0.013278" quat="0.482348 0.516286 -0.516862 0.483366" mass="1.1636" diaginertia="0.0113017 0.011088 0.00102532" />
                    <joint name="arm3/Actuator2" pos="0 0 0" axis="0 0 1" limited="true" range="-2.41 2.41" damping="0.1" />
                    <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" name="arm3/ha1_visual" mesh="half_arm_1_link" />
                    <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="half_arm_1_link" name="arm3/ha1_collision"/>
                    <body name="arm3/HalfArm3_Link" pos="0 -0.21038 -0.006375" quat="0.707105 -0.707108 0 0">
                        <inertial pos="-4.4e-05 -0.006641 -0.117892" quat="0.706144 0.0213722 -0.0209128 0.707437" mass="1.1636" diaginertia="0.0111633 0.010932 0.00100671" />
                        <joint name="arm3/Actuator3" pos="0 0 0" axis="0 0 1" damping="0.1" />
                        <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" name="arm3/ha2_visual" mesh="half_arm_2_link" />
                        <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="half_arm_2_link" name="arm3/ha2_collision"/>
                        <body name="arm3/forearm_link" pos="0 0.006375 -0.21038" quat="0.707105 0.707108 0 0">
                            <inertial pos="-1.8e-05 -0.075478 -0.015006" quat="0.483678 0.515961 -0.515859 0.483455" mass="0.9302" diaginertia="0.00834839 0.008147 0.000598606" />
                            <joint name="arm3/Actuator4" pos="0 0 0" axis="0 0 1" limited="true" range="-2.66 2.66" damping="0.1" />
                            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" name="arm3/f_visual" mesh="forearm_link" />
                            <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="forearm_link" name="arm3/f_collision"/>
                            <body name="arm3/SphericalWrist1_Link" pos="0 -0.20843 -0.006375" quat="0.707105 -0.707108 0 0">
                                <inertial pos="1e-06 -0.009432 -0.063883" quat="0.703558 0.0707492 -0.0707492 0.703558" mass="0.6781" diaginertia="0.00165901 0.001596 0.000346988" />
                                <joint name="arm3/Actuator5" pos="0 0 0" axis="0 0 1" damping="0.1" />
                                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" name="arm3/w1_visual" mesh="spherical_wrist_1_link" />
                                <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="spherical_wrist_1_link" name="arm3/w1_collision"/>
                                <body name="arm3/SphericalWrist2_Link" pos="0 0.00017505 -0.10593" quat="0.707105 0.707108 0 0">
                                    <inertial pos="1e-06 -0.045483 -0.00965" quat="0.44426 0.550121 -0.550121 0.44426" mass="0.6781" diaginertia="0.00170087 0.001641 0.00035013" />
                                    <joint name="arm3/Actuator6" pos="0 0 0" axis="0 0 1" limited="true" range="-2.23 2.23" damping="0.01" />
                                    <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" name="arm3/w2_visual" mesh="spherical_wrist_2_link" />
                                    <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="spherical_wrist_2_link" name="arm3/w2_collision"/>
                                    <body name="arm3/Bracelet_Link" pos="0 -0.10593 -0.00017505" quat="0.707105 -0.707108 0 0">
                                        <inertial pos="-0.000281 -0.011402 -0.029798" quat="0.601961 0.40663 -0.381169 0.571843" mass="0.5006" diaginertia="0.000657515 0.000586793 0.000320693" />
                                        <joint name="arm3/Actuator7" pos="0 0 0" axis="0 0 1" damping="0.01" />
                                        <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="1 1 1 1" name="arm3/b_visual" mesh="bracelet_with_vision_link" />
                                        <geom type="mesh" conaffinity="0" rgba="0.75294 0.75294 0.75294 1" mesh="bracelet_with_vision_link" name="arm3/b_collision"/>
                                        <body name="arm3/right_hand" pos="0 0 -0.065" quat="0 0.707105 -0.707108 0">
                                            <!-- This camera points out from the eef. -->
                                            <camera mode="fixed" name="arm3/eye_in_hand" pos="0.05 0 0" quat="0 0.707108 0.707108 0" fovy="75"/>
                                            <!-- To add gripper -->
                                        
                                                <body name="arm3/4boxes" pos="0 0 0.05" childclass="active_adhesion">
                                                    <site name="arm3/force_sensor" group="3"/>
                                                        <!-- <joint name="adj_ee" damping=".01" pos="0 0 0"/> -->
                                                        <geom type="box" friction=".5" size=".015 .015 .01" pos="0.015 0.015 0"/>
                                                        <geom type="box" friction=".5" size=".015 .015 .01" pos="0.015 -.015 0"/>
                                                        <geom type="box" friction=".5" size=".015 .015 .01" pos="-.015 0.015 0"/>
                                                        <geom type="box" friction=".5" size=".015 .015 .01" pos="-.015 -.015 0"/>
                                                </body>
                                        
                                        </body>
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

  <sensor>
    <force site="arm3/force_sensor"/>
  </sensor>



</mujoco>
